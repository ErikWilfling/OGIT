	dcterms:valid "start=2025-09-08;";
	dcterms:creator "Sebastion Seigfried";
	ogit:scope "NTO";
	ogit:parent ogit:Node;
	ogit:mandatory-attributes (
        ogit:id
	);
	ogit:optional-attributes (
        ogit:createdAt
        ogit:openedAt
        ogit:lastUpdatedAt
        ogit:closedAt
        ogit:summary
        ogit:description
        ogit:priority
        ogit:impact
        ogit:urgency
        ogit:project 
        ogit:source
        ogit:name
        ogit:category
