
@prefix ogit.Publications:      <http://www.purl.org/ogit/Publications/> .
@prefix ogit.Forum:      		<http://www.purl.org/ogit/Forum/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .

ogit.Publications:Publication
	a rdfs:Class;
	rdfs:subClassOf ogit:Entity;
	rdfs:label "Publication";
	dcterms:description "A public or private publication. Could be a newspaper article, book, or other.";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2015-04-27;";
	dcterms:creator "esteban.moreno@almato.de";
	ogit:scope "NTO";
	ogit:parent ogit:Node;
	ogit:mandatory-attributes (
		ogit:externalId
		ogit:content
	);
	ogit:optional-attributes (
		ogit:createdAt
		ogit:type
		ogit:webPage
		ogit.Publications:sourceDocumentIdentifier
		ogit:author
	);
	ogit:indexed-attributes (
		
	);
	ogit:allowed (
			[ ogit:connects  ogit.Forum:Topic ]
			[ ogit.Forum:mentions ogit:Count ]
			[ ogit.Forum:mentions ogit:Region ]
			[ ogit.Forum:mentions ogit:Person ]
			[ ogit.Forum:mentions ogit:Organization ]
			[ ogit:has ogit:Tone ]
			[ ogit:contains  ogit.Publications:Mention ]
	);
.
