@prefix ogit.GeoProfile:     <http://www.purl.org/ogit/GeoProfile/> .
@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .

ogit.GeoProfile:population
    a owl:DatatypeProperty;
    rdfs:subPropertyOf ogit:Attribute;
    rdfs:label "population";
    dcterms:description "Represents the average population for the most recent calendar year of a geo-political region. Historical population data should be stored in a separate time-series node, linked to the entity that contains this attribute";
    dcterms:valid "start=2025-08-12;";
    dcterms:creator "Calvin Spolwind";
.