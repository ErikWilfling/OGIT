@prefix ogit:      <http://www.purl.org/ogit/> .
@prefix owl:       <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:      <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:   <http://purl.org/dc/terms/> .

ogit:identifierCode
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf ogit:Attribute ;
    rdfs:label "identifierCode" ;
    dcterms:description """A standardized code that serves as an identifier, such as ISO codes, industry classification codes, or other reference codes.
							The type of code can be inferred from the associated codeType attribute on the node.""" ;
    dcterms:valid "start=2025-09-09;" ;
    dcterms:creator "Calvin Spolwind" ;
.